    "require": {
        "php": ">=7.4",
        "ext-openssl" : "*",
        "typo3/cms-core": "~9.5.0",
		"madj2k/t3-accelerator": "~9.5.2",
		"madj2k/t3-ajax-api": "~9.5.0",
